
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  UserID              Int     @id @default(autoincrement())
  Gender              String
  FirstName           String
  LastName            String
  Email               String
  PhoneNumber         String
  Role                String
  SSN                 String
  Password            String
  UserName            String  @unique
  InsurancePersentage Float?
  EmergencyContact    String?
  Specialization      String?
  ClinicID            Int?    
}
